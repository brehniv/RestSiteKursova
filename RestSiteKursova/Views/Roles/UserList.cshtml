@model IEnumerable<User>
@{
    ViewBag.Title = "Список користувачів | Козацька справа";
}
<div class="container">
<h2>Список користувачів</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="button" style="margin-left: 10px; padding:1px" asp-action="Edit" asp-route-userid="@user.Id">Права доступу</a>
            </td>
        </tr>
    }
</table>
</div>